    "react-dom": "^16.4.0",
    "react-draggable": "^3.0.5",
    "reactjs-popup": "^1.1.1",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "better-npm-run": "^0.1.0",
    "mini-css-extract-plugin": "^0.4.0",
